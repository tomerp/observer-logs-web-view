{
  "name": "observer-logs-web-view",
  "version": "0.1.0",
  "private": true,
  "description": "WebSocket + REST log tailer for ftso observer, with lightweight analytics",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "mkdir -p dist/static && cp -R src/static/. dist/static/",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"ts-node src/index.ts\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}


